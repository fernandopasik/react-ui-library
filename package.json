{
  "name": "react-ui-library",
  "version": "0.13.21",
  "description": "UI Component library made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --inline",
    "test": "jest --coverage",
    "lint": "eslint . && stylelint 'styleguide/*.scss' 'src/**/*.scss'",
    "build": "rm -rf dist && npm run build:js && npm run copy:scss",
    "prebuild": "npm run -s lint && npm test",
    "build:js": "babel src --out-dir dist --ignore spec.js",
    "copy:scss": "rsync -a --include '*/' --include '*.scss' --exclude '*' src/ dist/",
    "watch": "npm test -- -w & npm start",
    "deploy": "rm -rf deploy && webpack && gh-pages -d deploy",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "travis": "npm run -s build && npm run -s coveralls",
    "prepublishOnly": "npm run -s build",
    "preversion": "npm run -s prebuild",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run -s lint",
    "prepush": "npm run -s prebuild",
    "preparecommitmsg": "prepare-commit-msg-angular $2 $3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "mobile-detect": "^1.4.3",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-children-utilities": "^1.0.8",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.8",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^1.3.1",
    "hyperform": "^0.9.22",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "prepare-commit-msg-angular": "^0.2.0",
    "raf": "^3.4.1",
    "react-markdown-loader": "^1.2.0",
    "react-test-renderer": "^16.8.3",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.29.5",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "^4.2.1",
    "webpack-notifier": "^1.7.0"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  }
}
