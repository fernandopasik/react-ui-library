{
  "name": "react-ui-library",
  "version": "0.5.0",
  "description": "UI Component library boilerplate made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com.ar)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "",
  "scripts": {
    "start": "webpack-dev-server --open --inline",
    "lint": "eslint . && stylelint src/**/*.scss --syntax scss",
    "test": "nyc mocha",
    "pretest": "rm -rf coverage .nyc_output",
    "build": "npm run -s lint && npm test",
    "watch": "npm run test -- -w & npm start",
    "travis": "npm run -s build",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run -s lint",
    "prepush": "npm run -s lint && npm test",
    "preparecommitmsg": "prepare-commit-msg-angular $2 $3"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "require": [
      "babel-register"
    ],
    "include": [
      "src/"
    ],
    "exclude": [
      "src/utils/test.js",
      "src/**/*.spec.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chai-colors": "^1.0.1",
    "chai-enzyme": "^0.5.1",
    "cheerio": "^0.22.0",
    "css-loader": "^0.25.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.4.1",
    "eslint-config-fernandopasik": "^0.4.6",
    "eslint-loader": "^1.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "husky": "^0.11.7",
    "json-loader": "^0.5.4",
    "mobile-detect": "^1.3.3",
    "mocha": "^3.0.2",
    "node-sass": "^3.10.0",
    "nyc": "^8.3.0",
    "postcss-loader": "^0.13.0",
    "prepare-commit-msg-angular": "^0.2.0",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.3.1",
    "react-markdown-loader": "^1.1.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "stylelint-config-fernandopasik": "^0.1.1",
    "stylelint-webpack-plugin": "^0.3.1",
    "validate-commit-msg": "^2.8.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  }
}
