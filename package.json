{
  "name": "react-ui-library",
  "version": "0.13.18",
  "description": "UI Component library made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --inline",
    "lint": "eslint . && stylelint 'styleguide/*.scss' 'src/**/*.scss' --syntax scss",
    "test": "jest",
    "coverage": "jest --coverage",
    "check": "npm run -s lint && npm run -s coverage",
    "build": "npm run -s check && npm run build:js && npm run copy:scss",
    "prebuild": "rm -rf lib",
    "build:js": "babel src --out-dir lib --ignore spec.js",
    "copy:scss": "rsync -a --include '*/' --include '*.scss' --exclude '*' src/ lib/",
    "dist": "rm -rf dist && webpack",
    "deploy": "npm run -s dist && gh-pages -d dist",
    "prepublishOnly": "npm run -s build",
    "watch": "npm run test -- -w & npm start",
    "preversion": "npm run -s check",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run -s lint",
    "prepush": "npm run -s check",
    "preparecommitmsg": "prepare-commit-msg-angular $2 $3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/test/setup.sass.js"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/utils/",
      "<rootDir>/test/"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-children-utilities": "^0.1.12",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "eslint": "^4.0.0",
    "eslint-config-fernandopasik": "^0.8.2",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "hyperform": "^0.9.6",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.2.0",
    "json-loader": "^0.5.4",
    "mobile-detect": "^1.3.6",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "prepare-commit-msg-angular": "^0.2.0",
    "react-addons-test-utils": "^15.6.0",
    "react-markdown-loader": "^1.1.11",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "stylelint": "^7.11.1",
    "stylelint-config-fernandopasik": "^0.1.3",
    "stylelint-webpack-plugin": "^0.7.0",
    "validate-commit-msg": "^2.12.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  }
}
