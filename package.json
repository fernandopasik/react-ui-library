{
  "name": "react-ui-library",
  "version": "0.13.21",
  "description": "UI Component library made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --inline",
    "test": "jest --coverage",
    "lint": "eslint . && stylelint 'styleguide/*.scss' 'src/**/*.scss'",
    "build": "rm -rf dist && npm run build:js && npm run copy:scss",
    "prebuild": "npm run -s lint && npm test",
    "build:js": "babel src --out-dir dist --ignore spec.js",
    "copy:scss": "rsync -a --include '*/' --include '*.scss' --exclude '*' src/ dist/",
    "watch": "npm test -- -w & npm start",
    "deploy": "rm -rf deploy && webpack && gh-pages -d deploy",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "travis": "npm run -s build && npm run -s coveralls",
    "prepublishOnly": "npm run -s build",
    "preversion": "npm run -s prebuild",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run -s lint",
    "prepush": "npm run -s prebuild",
    "preparecommitmsg": "prepare-commit-msg-angular $2 $3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "mobile-detect": "^1.3.7",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-children-utilities": "^0.2.2",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "hyperform": "^0.9.9",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.7",
    "prepare-commit-msg-angular": "^0.2.0",
    "raf": "^3.4.0",
    "react-markdown-loader": "^1.1.14",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.1.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  }
}
