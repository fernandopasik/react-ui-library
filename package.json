{
  "name": "react-ui-library",
  "version": "0.1.0",
  "description": "UI Component library boilerplate made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com.ar)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "",
  "scripts": {
    "test": "nyc mocha",
    "pretest": "rm -rf coverage .nyc_output",
    "lint": "eslint .; stylelint src/**/*.scss --syntax scss;"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm run -s lint",
      "pre-push": "npm run -s lint && npm test",
      "prepare-commit-msg": "prepare-commit-msg-angular $2 $3"
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "require": [
      "babel-register"
    ],
    "exclude": [
      "src/utils/test.js",
      "src/**/*.spec.js",
      "test/",
      "coverage/"
    ],
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-colors": "^1.0.1",
    "cheerio": "^0.20.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.4.1",
    "eslint-config-fernandopasik": "^0.4.3",
    "ghooks": "^1.3.2",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "nyc": "^7.0.0",
    "prepare-commit-msg-angular": "^0.2.0",
    "react-addons-test-utils": "^15.2.1",
    "stylelint-config-fernandopasik": "^0.1.1",
    "validate-commit-msg": "^2.6.1"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.14.2"
  }
}
