{
  "name": "react-ui-library",
  "version": "0.13.11",
  "description": "UI Component library boilerplate made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --inline",
    "lint": "eslint . && stylelint 'styleguide/*.scss' 'src/**/*.scss' --syntax scss",
    "test": "mocha",
    "coverage": "rm -rf coverage .nyc_output && nyc mocha",
    "check": "npm run -s lint && npm run -s coverage",
    "dist": "rm -rf dist && webpack",
    "build": "npm run -s check && npm run -s dist",
    "prepublishOnly": "npm run -s dist",
    "watch": "npm run test -- -w & npm start",
    "preversion": "npm run -s check",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run -s lint",
    "prepush": "npm run -s check",
    "preparecommitmsg": "prepare-commit-msg-angular $2 $3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-children-utilities": "^0.1.6",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chai-colors": "^1.0.1",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "css-loader": "^0.27.3",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.7.1",
    "eslint-config-fernandopasik": "^0.5.1",
    "eslint-loader": "^1.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.2",
    "hyperform": "^0.9.5",
    "jsdom": "^9.12.0",
    "json-loader": "^0.5.4",
    "mobile-detect": "^1.3.5",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.1",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.3.3",
    "prepare-commit-msg-angular": "^0.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-markdown-loader": "^1.1.10",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "stylelint-config-fernandopasik": "^0.1.3",
    "stylelint-webpack-plugin": "^0.7.0",
    "validate-commit-msg": "^2.11.2",
    "webpack": "^2.3.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "webpack-notifier": "^1.5.0"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  }
}
