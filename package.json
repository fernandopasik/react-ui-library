{
  "name": "react-ui-library",
  "version": "0.13.5",
  "description": "UI Component library boilerplate made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --inline",
    "lint": "eslint . && stylelint 'styleguide/*.scss' 'src/**/*.scss' --syntax scss",
    "test": "mocha",
    "coverage": "rm -rf coverage .nyc_output && nyc mocha",
    "check": "npm run -s lint && npm run -s coverage",
    "dist": "rm -rf dist && webpack",
    "build": "npm run -s check && npm run -s dist",
    "prepublishOnly": "npm run -s dist",
    "watch": "npm run test -- -w & npm start",
    "preversion": "npm run -s check",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run -s lint",
    "prepush": "npm run -s check",
    "preparecommitmsg": "prepare-commit-msg-angular $2 $3"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "require": [
      "babel-register"
    ],
    "include": [
      "src/"
    ],
    "exclude": [
      "dist/**",
      "node_modules/**",
      "src/utils/test.js",
      "src/**/*.spec.js"
    ],
    "sourceMap": false,
    "instrument": false,
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-children-utilities": "^0.1.4",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-colors": "^1.0.1",
    "chai-enzyme": "^0.6.0",
    "cheerio": "^0.22.0",
    "css-loader": "^0.25.0",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.6.0",
    "eslint-config-fernandopasik": "^0.5.1",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "husky": "^0.11.9",
    "hyperform": "^0.8.11",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "mobile-detect": "^1.3.4",
    "mocha": "^3.1.2",
    "node-sass": "^3.11.2",
    "nyc": "^8.4.0",
    "postcss-loader": "^1.1.1",
    "prepare-commit-msg-angular": "^0.2.0",
    "react-addons-test-utils": "^15.3.2",
    "react-markdown-loader": "^1.1.6",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "stylelint-config-fernandopasik": "^0.1.2",
    "stylelint-webpack-plugin": "^0.4.0",
    "validate-commit-msg": "^2.8.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0",
    "webpack-notifier": "^1.4.1"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  }
}
