{
  "name": "react-ui-library",
  "version": "0.14.2",
  "description": "UI Component library made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --inline",
    "test": "jest --coverage",
    "lint": "eslint . && stylelint 'styleguide/*.scss' 'src/**/*.scss'",
    "build": "rm -rf dist && npm run build:js && npm run copy:scss",
    "prebuild": "npm run -s lint && npm test",
    "build:js": "babel src --out-dir dist --ignore spec.js",
    "copy:scss": "rsync -a --include '*/' --include '*.scss' --exclude '*' src/ dist/",
    "watch": "npm test -- -w & npm start",
    "deploy": "rm -rf deploy && webpack && gh-pages -d deploy",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run -s build",
    "preversion": "npm run -s prebuild",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run -s lint",
    "prepush": "npm run -s prebuild",
    "preparecommitmsg": "prepare-commit-msg-angular $2 $3"
  },
  "resolutions": {
    "underscore.string": "^3.3.5"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "mobile-detect": "^1.4.4",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-children-utilities": "^1.3.3",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "autoprefixer": "^9.6.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "husky": "^3.0.8",
    "hyperform": "^0.11.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prepare-commit-msg-angular": "^0.2.0",
    "raf": "^3.4.1",
    "react-markdown-loader": "^1.2.1",
    "react-test-renderer": "^16.10.2",
    "sass-loader": "^8.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.1",
    "stylelint-webpack-plugin": "^1.0.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2",
    "webpack-notifier": "^1.8.0"
  }
}
