{
  "name": "react-ui-library",
  "version": "0.2.0",
  "description": "UI Component library boilerplate made with React",
  "keywords": [],
  "homepage": "https://github.com/fernandopasik/react-ui-library",
  "bugs": "https://github.com/fernandopasik/react-ui-library/issues",
  "license": "MIT",
  "author": "Fernando Pasik <fernando@pasik.com.ar> (http://fernandopasik.com.ar)",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/fernandopasik/react-ui-library.git"
  },
  "main": "",
  "scripts": {
    "start": "webpack-dev-server",
    "lint": "eslint .; stylelint src/**/*.scss --syntax scss;",
    "test": "nyc mocha",
    "pretest": "rm -rf coverage .nyc_output",
    "build": "npm run -s lint && npm test",
    "travis": "npm run -s build"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg",
      "pre-commit": "npm run -s lint",
      "pre-push": "npm run -s lint && npm test",
      "prepare-commit-msg": "prepare-commit-msg-angular $2 $3"
    }
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "require": [
      "babel-register"
    ],
    "include": [
      "src/"
    ],
    "exclude": [
      "src/utils/test.js",
      "src/**/*.spec.js"
    ],
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.11.6",
    "chai": "^3.5.0",
    "chai-colors": "^1.0.1",
    "cheerio": "^0.22.0",
    "css-loader": "^0.23.1",
    "dirty-chai": "^1.2.2",
    "enzyme": "^2.4.1",
    "eslint-config-fernandopasik": "^0.4.4",
    "eslint-loader": "^1.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ghooks": "^1.3.2",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "mobile-detect": "^1.3.3",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "nyc": "^8.1.0",
    "postcss-loader": "^0.10.1",
    "prepare-commit-msg-angular": "^0.2.0",
    "react-addons-test-utils": "^15.3.1",
    "react-markdown-loader": "^1.1.2",
    "sass-loader": "^4.0.0",
    "stylelint-config-fernandopasik": "^0.1.1",
    "stylelint-webpack-plugin": "^0.3.1",
    "validate-commit-msg": "^2.8.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  }
}
